{
  "name": "persoo-autocomplete",
  "version": "2.5.2",
  "description": "Persoo autocomplete widget.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p 8080 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "postbuild": "mkdirp dist && ncp build/bundle.js dist/persooAutocomplete.js",
    "test": "npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test-watch": "karma start test/karma.conf.js --single-run=false --auto-watch"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Kuba Cerny <kuba@persoo.cz>",
  "devDependencies": {
    "babel": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-transform-decorators-legacy": "*",
    "babel-plugin-transform-react-jsx": "*",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "babel-runtime": "*",
    "chai": "*",
    "copy-webpack-plugin": "*",
    "core-js": "*",
    "cross-env": "*",
    "eslint": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "json-loader": "*",
    "karma": "*",
    "karma-chai": "*",
    "karma-chai-sinon": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "*",
    "mkdirp": "*",
    "mocha": "*",
    "ncp": "*",
    "offline-plugin": "^5.0.5",
    "path": "*",
    "phantomjs-prebuilt": "*",
    "raw-loader": "*",
    "sinon": "*",
    "sinon-chai": "*",
    "source-map-loader": "*",
    "style-loader": "*",
    "superstatic": "*",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "*",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "es6-symbol": "*",
    "lint": "^1.1.2",
    "persoo-templates": "*",
    "preact": "*",
    "preact-compat": "*",
    "preact-router": "*",
    "proptypes": "*"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  }
}
